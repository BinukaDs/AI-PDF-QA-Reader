{
  "version": 2,
  "functions": {
    "api/express.js": {
      "includeFiles": [
        "server/uploads/sample.pdf",
        "server/uploads/page-data.json"
      ]
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/express.js" },
    { "src": "/(.*)", "dest": "/frontend/dist/$1" }
  ]
}
